<Window x:Class="DiagramTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="http://oxyplot.codeplex.com"
        xmlns:DiagramTest="clr-namespace:DiagramTest"
        Title="MainWindow" Height="800" Width="1280" Icon="Arc_icon.png">
    <Window.DataContext>
        <DiagramTest:MainViewModel/>
    </Window.DataContext>
    
    <Grid>
        <oxy:Plot x:Name="TestPlot" Title="Graph Test" Grid.Column="1" Model="{Binding Model}" />      
    </Grid>
</Window>
